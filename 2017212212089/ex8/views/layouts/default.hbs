<body>
    <div>
        <div>
            <div>
                <form>
                    <div>
						<div>
							邮箱地址：
                            <input type="email" id="Email" placeholder="">
                            <button type="button" id="getCAPTCHA">获取验证码</button>
                        </div>
                        {{!-- <div class="alert alert-warning" role="alert"></div> --}}
                    </div>
                    <div class="form-group">
                        密码：
                        <input type="password" id="password" placeholder="">
                    </div>
                    <div>
                        再一次输入密码：
                        <input type="password" id="checkPassword" placeholder="">
                    </div>
                    <div>
                        验证码：
                        <input type="text" id="CAPTCHA" placeholder="">
                    </div>
                    <button type="button" id="register">提交</button>
                </form>
            </div>
        </div>
    </div>
</body>
